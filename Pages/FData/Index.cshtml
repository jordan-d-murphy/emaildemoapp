@page
@model emaildemoapp.Pages_FData.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FData[0].Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FData[0].Password)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FData[0].Address1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FData[0].Address2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FData[0].City)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FData[0].State)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FData[0].Zip)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FData[0].TextArea)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FData[0].Check1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FData[0].Check2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FData[0].Check3)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FData[0].SubmittedDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.FData) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Address1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Address2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.City)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.State)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Zip)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TextArea)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Check1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Check2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Check3)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SubmittedDate)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
